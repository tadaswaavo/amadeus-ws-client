  <xs:import namespace="http://xml.amadeus.com/2010/06/TicketTypes_v2" schemaLocation="../2015F/AMA_Ticket_GTP_CommonTypes_Proxy.xsd" />
  <xs:element name="AMA_TicketCancelTransactionRQ">
    <xs:annotation>
      <xs:documentation>Cancel Transaction client request message</xs:documentation>
    </xs:annotation>
    <xs:complexType>
      <xs:sequence>
        <xs:choice minOccurs="0">
          <xs:element name="Contract" maxOccurs="99">
            <xs:complexType>
              <xs:attribute name="Tattoo" type="xs:nonNegativeInteger" use="optional">
                <xs:annotation>
                  <xs:documentation>Tattoo of the entity in the PNR.</xs:documentation>
                </xs:annotation>
              </xs:attribute>
              <xs:attribute name="Number" type="ota:AlphaNumericStringLength1to14" />
              <xs:attribute name="SalesReportSequenceNumber" type="att:AlphaNumericString_Length1To35" />
            </xs:complexType>
          </xs:element>
          <xs:element name="AllContracts">
            <xs:annotation>
              <xs:documentation>Selects tickets per provider(s) specified. Only in PNR context</xs:documentation>
            </xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element name="Provider" type="ota:CompanyNameType" minOccurs="0" maxOccurs="9">
                  <xs:annotation>
                    <xs:documentation>If not specified then tickets from all providers are selected</xs:documentation>
                  </xs:annotation>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
        </xs:choice>
        <xs:element name="ActionDetail" minOccurs="0" maxOccurs="99">
