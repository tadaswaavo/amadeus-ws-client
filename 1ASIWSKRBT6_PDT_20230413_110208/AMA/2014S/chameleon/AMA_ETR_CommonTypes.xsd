    </xs:attribute>
    <xs:attribute name="stateCode" type="AlphaNumericStringLength1to2orString0" use="optional">
      <xs:annotation>
        <xs:appinfo source="http://xml.amadeus.com/ErrorCodes">
          <ama:error code="12701">INVALID STATE CODE</ama:error>
